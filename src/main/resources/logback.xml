<configuration>

    <appender name="console" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>[%logger{36}] - %msg%n</pattern>
        </encoder>
    </appender>

    <logger name="org.eeichinger" level="info" />
    
    <root level="warn">
        <appender-ref ref="console"/>
    </root>

</configuration>
